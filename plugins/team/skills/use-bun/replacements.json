  "ignores": [
    {
      "pattern": "node:cluster",
      "reason": "Cluster module not supported in Bun"
    },
    {
      "pattern": "node:worker_threads",
      "reason": "Bun uses Web Worker API - different interface than worker_threads"
    },
    {
      "pattern": "node:vm",
      "reason": "VM module has limited support in Bun"
    },
    {
      "pattern": "node:inspector",
      "reason": "Inspector module not fully supported"
    },
    {
      "pattern": "node:v8",
      "reason": "V8-specific module - Bun uses JavaScriptCore"
    },
    {
      "pattern": "node:async_hooks",
      "reason": "Async hooks only partially supported"
    },
    {
      "pattern": "node:trace_events",
      "reason": "Trace events not supported"
    },
    {
      "pattern": "node:diagnostics_channel",
      "reason": "Diagnostics channel only partially supported"
    },
    {
      "pattern": "node:crypto",
      "reason": "Bun's crypto compatibility is comprehensive - no replacement needed"
    },
    {
      "pattern": "chalk",
      "reason": "No native terminal coloring API (Bun.color is for CSS colors)"
    },
    {
      "pattern": "picocolors",
      "reason": "No native terminal coloring API"
    },
    {
      "pattern": "kleur",
      "reason": "No native terminal coloring API"
    },
    {
      "pattern": "ansi-styles",
      "reason": "No native terminal styling API"
    },
    {
      "pattern": "supports-color",
      "reason": "No native terminal color detection API"
    },
    {
      "pattern": "colorette",
      "reason": "No native terminal coloring API"
    },
    {
      "pattern": "ora",
      "reason": "No native spinner/progress API"
    },
    {
      "pattern": "cli-spinners",
      "reason": "No native spinner API"
