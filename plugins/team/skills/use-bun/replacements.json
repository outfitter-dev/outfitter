{
  "version": "1.1.0",
  "bunVersion": "1.3.0",
  "replacements": [
    {
      "id": "testing",
      "name": "Testing",
      "category": "testing",
      "bunApi": "bun:test",
      "docs": "https://bun.sh/docs/cli/test",
      "detectors": {
        "packages": ["jest", "vitest", "mocha", "ava", "tap", "uvu"],
        "imports": ["@jest/globals", "vitest"],
        "files": ["jest.config.*", "vitest.config.*", ".mocharc.*"]
      }
    },
    {
      "id": "bundling",
      "name": "Bundling",
      "category": "bundling",
